    $(function()
    {
       blist.namespace.fetch('blist.browse');
       blist.browse.datasets = {};
       blist.browse.baseURL = "/browse";
       blist.browse.rowCount = 3;
       blist.browse.searchOptions = JSON.parse($.unescapeQuotes("{&quot;limit&quot;:10,&quot;page&quot;:1,&quot;limitTo&quot;:&quot;apis&quot;}"));
