<!-- Inject number value into homepage API number -->
<script>
  $(document).ready(function(){
  	var url = '/api/search/views.json?limitTo=APIs';
  	var count = 0;
  	var name = "APIs";
  	$.getJSON(url, function(data){
  		count = data.count;
  		if (count == 1){
  			name = "API";
  		}
  		$('#apis').html($('<span>' + count + ' </span><small>' + name + '</small>'));
  	});
  });
</script>


